#include "main.h"
/**
* _strncpy - a function that compares two strings
* @s1: the 1st string.
* @s2: the 2nd string.
* Return: "0" if s2 = s1; "-" if s2 > s1; "+" if s2 < s1.
*/
int _strcmp(char *s1, char *s2)
{
int i = 0, l = 0;
while (s1[i] != '\0')
{
i++;
}
while (s2[l] != '\0')
{
l++;
}
return (i - l);
}
